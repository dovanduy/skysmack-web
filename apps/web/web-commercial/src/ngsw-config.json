{
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "/assets/icons/site.webmanifest",
          "/assets/icons/browserconfig.xml", 
          "/ngsw-worker.js",
          "/index.html",
          "/styles.*.css",
          "/main.*.js",
          "/runtime.*.js",
          "/polyfills.*.js"
        ],
        "urls": [
          "https://fonts.googleapis.com/**",
          "https://fonts.gstatic.com/**",
          "https://ajax.googleapis.com/ajax/libs/**",          
          "https://skysmack.com/**"
        ]
      }
    },
    {
      "name": "assets",
      "installMode": "lazy",
      "resources": {
        "files": [
          "/*.css",
          "/*.js",
          "/assets/**",
          "/*.(eot|svg|cur|jpg|png|webp|gif|otf|ttf|woff|woff2|ani)"
        ]
      }
    }
  ],
  "dataGroups": [
    {
      "name": "app",
      "urls": ["https://skysmack.com/*"],
      "cacheConfig": {
        "maxSize": "100",
        "maxAge": "90d",
        "strategy": "performance"
      }
    },
    {
      "name": "api",
      "urls": ["https://api.skysmack.com/*"],
      "cacheConfig": {
        "maxAge": "1h",
        "strategy": "freshness"
      }
    }
  ]
}