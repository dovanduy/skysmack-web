{
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "/index.html",
          "/ngsw-worker.js",
          "/assets/icons/**"
        ]
      }
    },
    {
      "name": "assets",
      "installMode": "lazy",
      "resources": {
        "files": [
          "/*.css",
          "/*.js",
          "/assets/**",
          "/*.(eot|svg|cur|jpg|png|webp|gif|otf|ttf|woff|woff2|ani)"
        ],
        "urls": [
          "https://fonts.googleapis.com/**",
          "https://ajax.googleapis.com/ajax/libs/hammerjs/**"
        ]
      }
    }
  ],
  "dataGroups": [
    {
      "name": "api",
      "urls": ["https://api.skysmack.com/*"],
      "cacheConfig": {
        "maxAge": "1d",
        "timeout": "500u",
        "strategy": "freshness"
      }
    }
  ]
}