{
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "/assets/icons/site.webmanifest",
          "/assets/icons/browserconfig.xml",          
          "/ngsw-worker.js",
          "/index.html"
        ],
        "urls": [
          "https://fonts.googleapis.com/**",
          "https://ajax.googleapis.com/ajax/libs/hammerjs/**",
          "https://cdn.skysmack.net/**"
        ]
      }
    }
  ],
  "dataGroups": [
    {
      "name": "cdn",
      "urls": ["https://cdn.skysmack.com/*"],
      "cacheConfig": {
        "maxSize": "100",
        "maxAge": "90d",
        "timeout": "50u",
        "strategy": "performance"
      }
    },
    {
      "name": "api",
      "urls": ["https://*.skysmack.io/*"],
      "cacheConfig": {
        "maxSize": "1000",
        "maxAge": "1d",
        "timeout": "500u",
        "strategy": "freshness"
      }
    }
  ]
}