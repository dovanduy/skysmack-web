{
  "name": "@skysmack/ng-skysmack",
  "version": "0.0.0",
  "scripts": {
    "link:framework": "cd ../framework && npm link && npm run build",
    "link:redux": "cd ../redux && npm link && npm link @skysmack/framework && npm run build",
    "link:packages-persons": "cd ../packages-persons && npm link && npm link @skysmack/framework @skysmack/redux && npm run build",
    "link:packages-skysmack": "cd ../packages-skysmack && npm link && npm run build",
    "link:ng-skysmack": "npm link @skysmack/framework @skysmack/redux @skysmack/packages-persons @skysmack/packages-skysmack",
    "ss:link": "run-s link:framework link:redux link:packages-persons link:packages-skysmack link:ng-skysmack",
    "serve:client1": "ng serve --host client1.skysmack.test --port 4000 --disable-host-check",
    "serve:client1:open": "ng serve --host client1.skysmack.test --port 4000 --open --disable-host-check",
    "build:client1:dev": "ng build --prod",
    "build:client1:prod": "ng build --prod --deploy-url //cdn.skysmack.net/",
    "ss": "run-s ss:link serve:client1",
    "ss:open": "run-s ss:link serve:client1:open",
    "ss:build": "run-s build:client1:prod"
  },
  "private": true,
  "dependencies": {
    "@angular-redux/store": "^9.0.0",
    "@angular-redux/router": "^9.0.0",
    "@angular/animations": "^7.0.2",
    "@angular/common": "^7.0.2",
    "@angular/compiler": "^7.0.2",
    "@angular/core": "^7.0.2",
    "@angular/forms": "^7.0.2",
    "@angular/http": "^7.0.2",
    "@angular/platform-browser": "^7.0.2",
    "@angular/platform-browser-dynamic": "^7.0.2",
    "@angular/router": "^7.0.2",
    "@redux-offline/redux-offline": "^2.4.0",
    "core-js": "^2.5.4",
    "redux": "^4.0.1",
    "redux-observable": "^1.0.0",
    "rxjs": "~6.3.3",
    "rxjs-compat": "^6.2.2",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.10.0",
    "@angular/cli": "~7.0.3",
    "@angular/compiler-cli": "^7.0.2",
    "@angular/language-service": "^7.0.2",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.3.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "npm-run-all": "^4.1.3",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "typescript": "~3.1.5",
    "wait-on": "^3.2.0"
  }
}