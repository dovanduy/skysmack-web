{
  "name": "@skysmack/ng-skysmack",
  "version": "0.0.0",
  "scripts": {
    "link:framework": "cd ../framework && npm link && npm run build",
    "link:redux": "cd ../redux && npm link && npm link @skysmack/framework && npm run build",
    "link:packages-authentication": "cd ../packages-authentication && npm link && npm-install-peers && npm link @skysmack/framework @skysmack/redux && npm run build",
    "link:packages-persons": "cd ../packages-persons && npm link && npm link @skysmack/framework @skysmack/redux && npm run build",
    "link:packages-products": "cd ../packages-products && npm link && npm link @skysmack/framework @skysmack/redux && npm run build",
    "link:packages-skysmack": "cd ../packages-skysmack && npm link && npm link @skysmack/framework && npm run build",
    "link:ng-skysmack": "npm link @skysmack/framework @skysmack/redux @skysmack/packages-authentication @skysmack/packages-persons  @skysmack/packages-products @skysmack/packages-skysmack",
    "build:c1:dev": "ng build --prod",
    "build:c1:prod": "ng build --prod --deploy-url //cdn.skysmack.net/",
    "start-server": "cd dist/web-portal && http-server -a www.skysmack.test -p 4000 -o",
    "ss:build": "run-s build:c1:prod",
    "ss:prod": "run-s ss:link  build:c1:dev start-server",
    "ss:c1": "ng serve --host client1.skysmack.test --port 4000 --disable-host-check",
    "ss:c1:open": "ng serve --host client1.skysmack.test --port 4000 --open --disable-host-check",
    "ss:init": "npm i && npm run ss:link && npm run ss:c1:open",
    "ss": "run-s ss:link ss:c1",
    "ss:open": "run-s ss:link ss:c1:open",
    "ss:clean": "clean.bat",
    "ss:rebuild": "npm run ss:clean && npm i && npm run ss",
    "ss:link": "run-s link:framework link:redux link:packages-authentication link:packages-persons link:packages-products link:packages-skysmack link:ng-skysmack"
  },
  "private": true,
  "dependencies": {
    "@angular-redux/router": "^9.0.0",
    "@angular-redux/store": "^9.0.0",
    "@angular/animations": "^7.0.2",
    "@angular/cdk": "^7.0.3",
    "@angular/common": "^7.0.2",
    "@angular/compiler": "^7.0.2",
    "@angular/core": "^7.0.2",
    "@angular/forms": "^7.0.2",
    "@angular/http": "^7.0.2",
    "@angular/material": "^7.0.3",
    "@angular/material-moment-adapter": "^7.0.3",
    "@angular/platform-browser": "^7.0.2",
    "@angular/platform-browser-dynamic": "^7.0.2",
    "@angular/pwa": "^0.10.6",
    "@angular/router": "^7.0.2",
    "@angular/service-worker": "^7.0.2",
    "@ngx-translate/core": "^11.0.0",
    "@ngx-translate/http-loader": "^3.0.1",
    "@redux-offline/redux-offline": "^2.4.0",
    "angular-calendar": "^0.26.4",
    "core-js": "^2.5.4",
    "date-fns": "^1.29.0",
    "hammerjs": "^2.0.8",
    "moment": "^2.22.2",
    "parse-link-header": "^1.0.1",
    "redux": "^4.0.1",
    "redux-observable": "^1.0.0",
    "rxjs": "~6.3.3",
    "rxjs-compat": "^6.2.2",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.10.0",
    "@angular/cli": "~7.0.3",
    "@angular/compiler-cli": "^7.0.2",
    "@angular/language-service": "^7.0.2",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.3.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "lite-server": "^2.4.0",
    "normalize-scss": "^7.0.1",
    "npm-run-all": "^4.1.3",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "typescript": "~3.1.5",
    "wait-on": "^3.2.0"
  }
}
