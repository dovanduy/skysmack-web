{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./../",
    "paths": {
      "@skysmack/ng-redux": [
        "dist/ng-redux"
      ],
      "@skysmack/ng-ui": [
        "dist/ng-ui"
      ],
      "@skysmack/ng-packages": [
        "dist/ng-packages"
      ],
      "@skysmack/portal-ui": [
        "dist/portal-ui"
      ],
      "@skysmack/portal-packages": [
        "dist/portal-packages"
      ]
    },
    "outDir": "./../dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "module": "es2015",
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "downlevelIteration": true,
    "target": "es5",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2018",
      "dom"
    ]
  }
}