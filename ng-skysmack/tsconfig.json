{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "src",
    "paths": {
      "ng-packages": [
        "dist/ng-packages"
      ],
      "ng-packages/*": [
        "dist/ng-packages/*"
      ],
      "portal-packages": [
        "dist/portal-packages"
      ],
      "portal-packages/*": [
        "dist/portal-packages/*"
      ],
      "portal-ui": [
        "dist/portal-ui"
      ],
      "portal-ui/*": [
        "dist/portal-ui/*"
      ],
      "ng-redux": [
        "dist/ng-redux"
      ],
      "ng-redux/*": [
        "dist/ng-redux/*"
      ],
      "ng-skysmack-core": [
        "dist/ng-skysmack-core"
      ],
      "ng-skysmack-core/*": [
        "dist/ng-skysmack-core/*"
      ]
    },
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "module": "es2015",
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "downlevelIteration": true,
    "target": "es5",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2018",
      "dom"
    ]
  }
}