{
  "name": "skysmack",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "node --max_old_space_size=8000 ./node_modules/@angular/cli/bin/ng",
    "start": "ng serve web-portal --host client1.skysmack.test --port 4000 --disableHostCheck",
    "start:prod": "ng build --prod --sourceMap=true && http-server ./dist/apps/web/web-portal -a www.skysmack.test -p 4000 -o",
    "build": "gulp webLocalization && ng build --prod --deploy-url //cdn.skysmack.net/",
    "build:dev": "ng build --stats-json && npm run local-dev-server-insecure",
    "build:prod": "ng build --prod --stats-json && npm run local-dev-server-insecure",
    "build:analyze": " webpack-bundle-analyzer ./dist/apps/web/web-portal/stats.json",
    "build:prod:analyze": "ng build --prod --stats-json && webpack-bundle-analyzer ./dist/apps/web/web-portal/stats.json",
    "local-dev-server": "cd ./tools/certificates && http-server ./../../dist/apps/web/web-portal -a www.skysmack.test -p 4000 -o -S -C skysmack-test-cert.pem",
    "local-dev-server-insecure": "cd ./tools/certificates && http-server ./../../dist/apps/web/web-portal -a www.skysmack.test -p 4000 -o",
    "test": "ng test",
    "lint": "./node_modules/.bin/nx lint && ng lint",
    "e2e": "ng e2e",
    "affected:apps": "./node_modules/.bin/nx affected:apps",
    "affected:libs": "./node_modules/.bin/nx affected:libs",
    "affected:build": "./node_modules/.bin/nx affected:build",
    "affected:e2e": "./node_modules/.bin/nx affected:e2e",
    "affected:test": "./node_modules/.bin/nx affected:test",
    "affected:lint": "./node_modules/.bin/nx affected:lint",
    "affected:dep-graph": "./node_modules/.bin/nx affected:dep-graph",
    "affected": "./node_modules/.bin/nx affected",
    "format": "./node_modules/.bin/nx format:write",
    "format:write": "./node_modules/.bin/nx format:write",
    "format:check": "./node_modules/.bin/nx format:check",
    "build:libs": "yarn build:libs:shared && yarn build:libs:packages",
    "build:libs:shared": "yarn libs:shared:framework:build && yarn libs:shared:framework:build && yarn libs:shared:redux:build && yarn libs:shared:signal-r:build && yarn libs:shared:pricings:build",
    "build:libs:packages": "yarn libs:packages:skysmack-core:build && yarn libs:packages:oauth2:build && yarn libs:packages:identities:build && yarn libs:packages:persons:build && yarn libs:packages:lodgings:build && yarn libs:packages:products:build && yarn libs:packages:invoices:build && yarn libs:packages:invoices-cash-payments:build && yarn libs:packages:invoices-products:build && yarn libs:packages:maintenance:build && yarn libs:packages:products-pricings:build && yarn libs:packages:lodging-reservations:build && yarn libs:packages:reservations-pricings:build && yarn libs:packages:persons-lodging-reservations:build && yarn libs:packages:terminal-payments:build && yarn libs:packages:emails:build && yarn libs:packages:emails-smtp:build",
    "libs:shared:redux:build": "node libs/shared/redux/ng-packagr-api.js",
    "libs:shared:pricings:build": "node libs/shared/pricings/ng-packagr-api.js",
    "libs:shared:signal-r:build": "node libs/shared/signal-r/ng-packagr-api.js",
    "libs:packages:skysmack-core:build": "node libs/packages/skysmack-core/ng-packagr-api.js",
    "libs:packages:identities:build": "node libs/packages/identities/ng-packagr-api.js",
    "libs:packages:invoices:build": "node libs/packages/invoices/ng-packagr-api.js",
    "libs:packages:invoices-products:build": "node libs/packages/invoices-products/ng-packagr-api.js",
    "libs:packages:invoices-cash-payments:build": "node libs/packages/invoices-cash-payments/ng-packagr-api.js",
    "libs:packages:lodging-reservations:build": "node libs/packages/lodging-reservations/ng-packagr-api.js",
    "libs:packages:lodgings:build": "node libs/packages/lodgings/ng-packagr-api.js",
    "libs:packages:maintenance:build": "node libs/packages/maintenance/ng-packagr-api.js",
    "libs:packages:oauth2:build": "node libs/packages/oauth2/ng-packagr-api.js",
    "libs:packages:persons:build": "node libs/packages/persons/ng-packagr-api.js",
    "libs:packages:persons-lodging-reservations:build": "node libs/packages/persons-lodging-reservations/ng-packagr-api.js",
    "libs:packages:products:build": "node libs/packages/products/ng-packagr-api.js",
    "libs:packages:products-pricings:build": "node libs/packages/products-pricings/ng-packagr-api.js",
    "libs:packages:reservations-pricings:build": "node libs/packages/reservations-pricings/ng-packagr-api.js",
    "libs:packages:terminal-payments:build": "node libs/packages/terminal-payments/ng-packagr-api.js",
    "libs:packages:emails:build": "node libs/packages/emails/ng-packagr-api.js",
    "libs:packages:emails-smtp:build": "node libs/packages/emails-smtp/ng-packagr-api.js",
    "libs:shared:framework:build": "ng-packagr -p libs/shared/framework/package.json",
    "libs:shared:package-types:build": "ng-packagr -p libs/shared/package-types/package.json",
    "gulp:localization": "gulp webLocalization",
    "update": "ng update @nrwl/schematics",
    "update:check": "ng update",
    "workspace-schematic": "./node_modules/.bin/nx workspace-schematic",
    "dep-graph": "./node_modules/.bin/nx dep-graph",
    "help": "./node_modules/.bin/nx help"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "8.1.2",
    "@angular/common": "8.1.2",
    "@angular/compiler": "8.1.2",
    "@angular/core": "8.1.2",
    "@angular/forms": "8.1.2",
    "@angular/platform-browser": "8.1.2",
    "@angular/platform-browser-dynamic": "8.1.2",
    "@angular/router": "8.1.2",
    "core-js": "2.6.8",
    "rxjs": "6.5.2",
    "zone.js": "0.9.1",
    "@angular-redux/router": "10.0.0",
    "@angular-redux/store": "10.0.0",
    "@angular/cdk": "8.1.1",
    "@angular/material": "8.1.1",
    "@angular/material-moment-adapter": "8.1.1",
    "@angular/pwa": "0.801.2",
    "@angular/service-worker": "8.1.2",
    "@aspnet/signalr": "1.1.4",
    "@kolkov/angular-editor": "0.17.1",
    "@ngx-translate/core": "11.0.1",
    "@redux-offline/redux-offline": "2.5.2",
    "angular-calendar": "0.27.14",
    "calendar-utils": "0.3.1",
    "guid-typescript": "1.0.9",
    "gulp": "4.0.2",
    "latinize": "^0.4.0",
    "localforage": "1.7.3",
    "moment": "2.24.0",
    "node-sass": "4.12.0",
    "parse-link-header": "1.0.1",
    "redux": "4.0.1",
    "redux-observable": "1.1.0",
    "swagger-ui": "3.23.1"
  },
  "devDependencies": {
    "@nrwl/cypress": "8.2.0",
    "jest-preset-angular": "7.1.1",
    "@angular/compiler-cli": "8.1.2",
    "@angular/language-service": "8.1.2",
    "@angular-devkit/build-angular": "0.801.2",
    "codelyzer": "5.1.0",
    "@nrwl/jest": "8.3.0",
    "@nrwl/schematics": "8.3.0",
    "@nrwl/builders": "8.0.2",
    "jest": "24.8.0",
    "@types/jest": "24.0.15",
    "ts-jest": "24.0.2",
    "@angular-devkit/build-ng-packagr": "0.801.2",
    "@angular/cli": "8.1.2",
    "@types/node": "12.6.8",
    "cypress": "3.4.0",
    "dotenv": "8.0.0",
    "gulp-merge-json": "1.3.1",
    "gulp-plumber": "1.2.1",
    "ng-packagr": "5.3.0",
    "normalize-scss": "7.0.1",
    "prettier": "1.18.2",
    "ts-node": "8.3.0",
    "tsickle": "0.36.0",
    "tslib": "1.10.0",
    "tslint": "5.18.0",
    "typescript": "3.4.5",
    "webpack-bundle-analyzer": "3.3.2"
  }
}
