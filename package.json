{
  "name": "skysmack",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "node --max_old_space_size=8000 ./node_modules/@angular/cli/bin/ng",
    "start": "ng serve web-portal --host client1.skysmack.test --port 4000 --disableHostCheck",
    "build": "gulp webLocalization && ng build --prod --deploy-url //cdn.skysmack.net/",
    "build:dev": "ng build --stats-json && npm run local-dev-server",
    "build:analyze": " webpack-bundle-analyzer ./dist/apps/web/web-portal/stats.json",
    "local-dev-server": "cd ./tools/certificates && http-server ./../../dist/apps/web/web-portal -a www.skysmack.test -p 4000 -o -S -C skysmack-test-cert.pem",
    "test": "ng test",
    "lint": "./node_modules/.bin/nx lint && ng lint",
    "e2e": "ng e2e",
    "affected:apps": "./node_modules/.bin/nx affected:apps",
    "affected:libs": "./node_modules/.bin/nx affected:libs",
    "affected:build": "./node_modules/.bin/nx affected:build",
    "affected:e2e": "./node_modules/.bin/nx affected:e2e",
    "affected:test": "./node_modules/.bin/nx affected:test",
    "affected:lint": "./node_modules/.bin/nx affected:lint",
    "affected:dep-graph": "./node_modules/.bin/nx affected:dep-graph",
    "affected": "./node_modules/.bin/nx affected",
    "format": "./node_modules/.bin/nx format:write",
    "format:write": "./node_modules/.bin/nx format:write",
    "format:check": "./node_modules/.bin/nx format:check",
    "build:libs": "yarn build:libs:shared && yarn build:libs:packages",
    "build:libs:shared": "yarn libs:shared:framework:build && yarn libs:shared:redux:build && yarn libs:shared:pricings:build",
    "build:libs:packages": "yarn libs:packages:skysmack-core:build && yarn libs:packages:oauth2:build && yarn libs:packages:identities:build && yarn libs:packages:account:build && yarn libs:packages:persons:build && yarn libs:packages:lodgings:build && yarn libs:packages:products:build && yarn libs:packages:invoices:build && libs:packages:invoices-cash-payments:build && yarn libs:packages:maintenance:build && yarn libs:packages:products-pricings:build && yarn libs:packages:lodging-reservations:build && yarn libs:packages:reservations-pricings:build && yarn libs:packages:persons-lodging-reservations:build && yarn libs:packages:terminal-payments:build ",
    "libs:shared:redux:build": "node libs/shared/redux/ng-packagr-api.js",
    "libs:shared:pricings:build": "node libs/shared/pricings/ng-packagr-api.js",
    "libs:packages:skysmack-core:build": "node libs/packages/skysmack-core/ng-packagr-api.js",
    "libs:packages:account:build": "node libs/packages/account/ng-packagr-api.js",
    "libs:packages:identities:build": "node libs/packages/identities/ng-packagr-api.js",
    "libs:packages:invoices:build": "node libs/packages/invoices/ng-packagr-api.js",
    "libs:packages:invoices-cash-payments:build": "node libs/packages/invoices-cash-payments/ng-packagr-api.js",
    "libs:packages:lodging-reservations:build": "node libs/packages/lodging-reservations/ng-packagr-api.js",
    "libs:packages:lodgings:build": "node libs/packages/lodgings/ng-packagr-api.js",
    "libs:packages:maintenance:build": "node libs/packages/maintenance/ng-packagr-api.js",
    "libs:packages:oauth2:build": "node libs/packages/oauth2/ng-packagr-api.js",
    "libs:packages:persons:build": "node libs/packages/persons/ng-packagr-api.js",
    "libs:packages:persons-lodging-reservations:build": "node libs/packages/persons-lodging-reservations/ng-packagr-api.js",
    "libs:packages:products:build": "node libs/packages/products/ng-packagr-api.js",
    "libs:packages:products-pricings:build": "node libs/packages/products-pricings/ng-packagr-api.js",
    "libs:packages:reservations-pricings:build": "node libs/packages/reservations-pricings/ng-packagr-api.js",
    "libs:packages:terminal-payments:build": "node libs/packages/terminal-payments/ng-packagr-api.js",
    "libs:shared:framework:build": "ng-packagr -p libs/shared/framework/package.json",
    "gulp:localization": "gulp webLocalization",
    "update": "ng update @nrwl/schematics",
    "update:check": "ng update",
    "workspace-schematic": "./node_modules/.bin/nx workspace-schematic",
    "dep-graph": "./node_modules/.bin/nx dep-graph",
    "help": "./node_modules/.bin/nx help"
  },
  "private": true,
  "dependencies": {
    "@angular-redux/router": "^10.0.0",
    "@angular-redux/store": "^10.0.0",
    "@angular/animations": "^8.0.0",
    "@angular/cdk": "^7.3.7",
    "@angular/common": "^8.0.0",
    "@angular/compiler": "^8.0.0",
    "@angular/core": "^8.0.0",
    "@angular/forms": "^8.0.0",
    "@angular/material": "^7.3.7",
    "@angular/material-moment-adapter": "^7.3.7",
    "@angular/platform-browser": "^8.0.0",
    "@angular/platform-browser-dynamic": "^8.0.0",
    "@angular/router": "^8.0.0",
    "@angular/pwa": "^0.13.9",
    "@angular/service-worker": "^8.0.0",
    "@ngx-translate/core": "^11.0.1",
    "@redux-offline/redux-offline": "^2.5.1",
    "angular-calendar": "^0.27.8",
    "calendar-utils": "^0.3.1",
    "core-js": "^2.6.8",
    "guid-typescript": "^1.0.9",
    "gulp": "^4.0.2",
    "localforage": "^1.7.3",
    "moment": "^2.24.0",
    "node-sass": "^4.12.0",
    "parse-link-header": "^1.0.1",
    "redux": "^4.0.1",
    "redux-observable": "^1.1.0",
    "rxjs": "^6.5.2",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.800.0",
    "@angular-devkit/build-ng-packagr": "~0.800.0",
    "@angular/cli": "8.0.0",
    "@angular/compiler-cli": "~8.0.0",
    "@angular/language-service": "^8.0.0",
    "@nrwl/builders": "7.8.6",
    "@nrwl/schematics": "7.8.6",
    "@types/jest": "24.0.13",
    "@types/node": "~12.0.2",
    "codelyzer": "^5.0.1",
    "cypress": "3.3.1",
    "dotenv": "8.0.0",
    "gulp-merge-json": "^1.3.1",
    "gulp-plumber": "^1.2.1",
    "jest": "24.8.0",
    "jest-preset-angular": "7.1.1",
    "ng-packagr": "^5.2.0",
    "normalize-scss": "^7.0.1",
    "prettier": "1.17.1",
    "ts-jest": "24.0.2",
    "ts-node": "~8.2.0",
    "tsickle": ">=0.35.0",
    "tslib": "^1.9.3",
    "tslint": "~5.16.0",
    "typescript": "3.4.5",
    "webpack-bundle-analyzer": "^3.3.2"
  }
}
